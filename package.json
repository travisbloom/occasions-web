{
    "name": "occasions-web",
    "version": "1.0.0",
    "description": "",
    "main": "webpack.config.js",
    "scripts": {
        "start": "BABEL_ENV=dev webpack-dev-server",
        "test": "jest --runInBand",
        "test:full": "jest",
        "lint": "eslint ./src",
        "format": "prettier --write \"src/**/*.js\"",
        "schema": "npm run server-schema && npm run flow-schema",
        "flow-schema": "apollo-codegen generate src/**/*.graphql --schema .tmp/schema.json --target flow --output src/types/schema.js",
        "server-schema": "apollo-codegen introspect-schema http://localhost:8000/graphql --output .tmp/schema.json"
    },
    "author": "travisbloom@gmail.com",
    "license": "UNLICENSED",
